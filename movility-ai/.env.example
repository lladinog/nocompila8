# MovilityAI Environment Configuration
# Copia este archivo a .env y configura tus credenciales

# Google Cloud Configuration
GOOGLE_CLOUD_PROJECT=tu-proyecto-gcp
GOOGLE_CLOUD_LOCATION=global
GOOGLE_GENAI_USE_VERTEXAI=True

# Google Maps API
GOOGLE_MAPS_API_KEY=tu-api-key-aqui

# Moovit API (opcional)
MOOVIT_API_KEY=tu-moovit-key-aqui

# Configuraci√≥n del sistema
LOG_LEVEL=INFO
ENABLE_CACHE=True

# === Route Planner Agent ===
OSRM_BASE_URL=https://routing.openstreetmap.de
OSRM_PROFILE_CAR=/routed-car
OSRM_PROFILE_BIKE=/routed-bike
OSRM_PROFILE_FOOT=/routed-foot

OPENWEATHER_API_KEY=YOUR_KEY_HERE
METRO_GTFS_URL=https://datosabiertos-metrodemedellin.opendata.arcgis.com/api/download/v1/items/0557214a4b5c4a1d90d36eb97ba54249/relationships?format=zip
ENCICLA_STATIONS_URL=https://www.datos.gov.co/resource/hmuf-kqju.json
